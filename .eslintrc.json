{
    "extends": "airbnb",
    "rules": {
        "linebreak-style": "off",
        "no-console": "off",
        "no-else-return": "off",
        "no-unused-vars": "off",
        "import/no-dynamic-require": "off",
        "import/first": "off",
        "consistent-return": "off",
        "strict": "off",
        "no-shadow": "off"
    },
    "env": {
        "mocha": true
    }
}